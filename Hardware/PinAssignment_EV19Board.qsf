set_location_assignment PIN_R13	-to	KEYBOARD_CLK					
					
set_instance_assignment -name IO_STANDARD "3.3-V LVTTL" -to  KEYBOARD_DATA
set_instance_assignment -name IO_STANDARD "3.3-V LVTTL" -to  KEYBOARD_CLK

