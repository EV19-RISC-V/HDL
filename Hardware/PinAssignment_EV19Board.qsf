#============================================================
# 							   UART
#============================================================
set_location_assignment PIN_J13	-to	UART_CTS				
set_location_assignment PIN_J16	-to	UART_TX				
set_location_assignment PIN_M10	-to	UART_RX				
set_location_assignment PIN_L14	-to	UART_DTR				
					
set_instance_assignment -name IO_STANDARD "3.3-V LVTTL" -to  UART_CTS
set_instance_assignment -name IO_STANDARD "3.3-V LVTTL" -to  UART_TX	
set_instance_assignment -name IO_STANDARD "3.3-V LVTTL" -to  UART_RX	
set_instance_assignment -name IO_STANDARD "3.3-V LVTTL" -to  UART_DTR

